<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/ww_hunter?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">2005</property>
    <mapping resource="bj/wwHunter/v1/model/Gameevents.hbm.xml"/>
    <mapping resource="bj/wwHunter/v1/model/Penaltys.hbm.xml"/>
    <mapping resource="bj/wwHunter/v1/model/Messages.hbm.xml"/>
    <mapping resource="bj/wwHunter/v1/model/News.hbm.xml"/>
    <mapping resource="bj/wwHunter/v1/model/Gamers.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
